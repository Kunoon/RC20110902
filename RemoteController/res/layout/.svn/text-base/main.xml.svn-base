<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:koobest="http://schemas.android.com/apk/res/com.koobest.customization"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:id="@+id/shelf_head" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:adjustViewBounds="true" />
	<com.koobest.customization.MyGridView
		android:scrollbars="none" android:id="@+id/mygridview"
		android:layout_width="match_parent" android:layout_height="match_parent"></com.koobest.customization.MyGridView>
</LinearLayout>
