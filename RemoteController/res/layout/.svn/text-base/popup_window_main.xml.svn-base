<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical" android:background="@drawable/dirbackground">
<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:layout_alignParentBottom="true">
<ImageView android:id="@+id/img_back" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:src="@drawable/icon_back" android:background="#00000000" />
<ImageView android:id="@+id/img_cancel"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_weight="1" android:src="@drawable/icon_close"
			android:background="#00000000" />
</LinearLayout>
<ListView android:id="@+id/dm_pop_listview"
		android:layout_width="fill_parent" android:layout_height="50dp"
		android:layout_weight="1" android:cacheColorHint="#00000000"
		android:background="#00000000"></ListView>
</LinearLayout>